{% extends 'base.html.twig' %}

{% block title %}{{ 'actor.index.title'|trans }}{% endblock %}

{% block body %}
<h1>{{ 'actor.index.heading'|trans }} :</h1>

<ul>
{% for actor in actors %}
    <li>
        <a href="{{ path("actor_show", { id: actor.id }) }}">{{ actor.name }}</a>
    </li>
{% endfor %}
</ul>
{% endblock %}
